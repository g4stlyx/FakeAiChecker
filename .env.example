{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "FakeAiChecker": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=FakeAiChecker;Uid=root;Pwd=your_password_here;"
  },
  "Security": {
    "MaxUploadSizeMB": 50,
    "AllowedFileExtensions": [
      ".txt", ".doc", ".docx", ".pdf", ".zip", ".rar", ".7z", ".tar", ".gz",
      ".env", ".json", ".xml", ".yml", ".yaml", ".config", ".ini", ".properties"
    ],
    "ScanTimeoutMinutes": 5
  }
}
